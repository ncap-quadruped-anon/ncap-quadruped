    <touch name="FR_foot" site="FR_foot_site"/>
    <touch name="FL_foot" site="FL_foot_site"/>
    <touch name="RR_foot" site="RR_foot_site"/>
    <touch name="RL_foot" site="RL_foot_site"/>
